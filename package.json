{
  "name": "slack-auto-status",
  "author": "Benjamin Pearson",
  "version": "1.0.0",
  "description": "Automatically update slack status based on nearby WiFi networks. Additionally the ability to manually update slack status from the command line.",
  "main": "app.js",
  "license": "MIT",
  "engines": {
    "node": "7.9.0"
  },
  "scripts": {
    "daemon:start": "pm2 start ecosystem.config.js",
    "daemon:stop": "pm2 stop ecosystem.config.js",
    "daemon:logs": "pm2 logs slack-auto-status",
    "scan": "node app.js scan"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "lowdb": "0.16.2",
    "ms": "1.0.0",
    "nconf": "0.8.4",
    "node-wifi": "1.2.4",
    "winston": "2.3.1",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminpearson/slack-auto-status.git"
  },
  "bugs": {
    "url": "https://github.com/benjaminpearson/slack-auto-status/issues"
  },
  "homepage": "https://github.com/benjaminpearson/slack-auto-status#readme"
}
